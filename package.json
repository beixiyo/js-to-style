{
  "name": "@jl-org/js-to-style",
  "version": "1.0.0",
  "description": "自动解析 TypeScript 主题对象文件并生成 CSS/SCSS/LESS 样式文件的工具包",
  "private": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "type-check": "tsc --noEmit",
    "test": "node examples/jsToStyle.cjs"
  },
  "dependencies": {
    "@babel/parser": "^7.28.4",
    "@babel/traverse": "^7.28.4",
    "@babel/types": "^7.28.4"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.28.0",
    "vite": "6.3.6",
    "vite-plugin-dts": "^4.5.4"
  },
  "keywords": [
    "js-to-style"
  ],
  "author": "CJL <2662442385@qq.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/beixiyo/js-to-style"
}